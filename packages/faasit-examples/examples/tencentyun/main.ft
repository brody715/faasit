@function helloTencentyun {
  runtime = "Nodejs12.16"
  codeDir = "./code"
  triggers = [
    {
      name = "http"
      kind = "http"
    }
  ]
  resource = {
    cpu = "1024m"
    memory = "128Mi"
  }
  events = []
}

@application {
  functions = [helloTencentyun]
  defaultProvider = TencentyunDefaultProvider
}

@provider TencentyunDefaultProvider {
  kind = "tencentyun"
}
