{
  "name": "faasit",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "description": "bundle for @faasit/cli",
  "engines": {
    "node": ">=12.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {}
  },
  "bin": {
    "ft": "./bin/faasit.cjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@faasit/cli": "workspace:^",
    "@faasit/benchmark": "workspace:^"
  }
}