{
    "name": "faasit",
    "displayName": "FaasIt Language Support",
    "description": "Language support for FaasIt",
    "version": "0.0.1",
    "publisher": "brody715",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/brody715/faasit.git"
    },
    "bugs": {
        "url": "https://github.com/brody715/faasit/issues"
    },
    "engines": {
        "vscode": "^1.67.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:faasit"
    ],
    "main": "out/extension",
    "contributes": {
        "languages": [
            {
                "id": "faasit",
                "aliases": [
                    "faasit"
                ],
                "extensions": [
                    ".ft"
                ],
                "configuration": "./faasit.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "faasit",
                "scopeName": "text.faasit",
                "path": "./syntaxes/faasit.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "faasit.a.proxy",
                "title": "Faasit Command A"
            },
            {
                "command": "faasit.b",
                "title": "Faasit Command B"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^17.0.34",
        "@types/vscode": "^1.67.0",
        "typescript": "^4.6.4",
        "@vscode/test-electron": "^2.1.3"
    },
    "dependencies": {
        "vscode-languageclient": "^8.0.1"
    },
    "scripts": {
        "prepublish": "tsc -p ./src",
        "compile": "tsc -p ./src",
        "watch": "tsc -w -p ./src",
        "update-vscode": "node ./node_modules/vscode/bin/install"
    }
}